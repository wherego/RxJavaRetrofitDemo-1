<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dagaozi.rxjavaretrofitdemo" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Base.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OperatorActivity"
            android:label="@string/title_activity_operator" >
        </activity>
        <activity
            android:name=".RecycleViewActivity"
            android:label="@string/title_activity_recycle_view" >
        </activity>
        <activity
            android:name=".RefreshRecyclerViewActivity"
            android:label="@string/title_activity_refresh_recycler_view" >
        </activity>
    </application>

</manifest>
